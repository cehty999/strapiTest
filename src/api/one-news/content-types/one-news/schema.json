{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "one-news",
    "pluralName": "news",
    "displayName": "News",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "likes": {
      "type": "biginteger",
      "default": "0"
    },
    "titleImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "article": {
      "type": "dynamiczone",
      "components": [
        "author.author",
        "header.header",
        "images.images",
        "list.list",
        "numerate.numerate",
        "quote.quote",
        "text.text",
        "sign.sign",
        "video.video"
      ]
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type.type"
    },
    "newsId": {
      "type": "uid"
    }
  }
}
